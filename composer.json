{
  "name": "ekut/mediawiki-sdk-php",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Yakutskiy",
      "email": "a.yakutskiy@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ekut/spatie-dto-validators": "^1.0",
    "guzzlehttp/guzzle": "^7.4",
    "spatie/data-transfer-object": "^3.7",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MediawikiSdkPhp\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
